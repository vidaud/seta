{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "seta-web": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/seta-web",
      "sourceRoot": "projects/seta-web/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/seta-web",
            "index": "projects/seta-web/src/index.html",
            "main": "projects/seta-web/src/main.ts",
            "polyfills": "projects/seta-web/src/polyfills.ts",
            "tsConfig": "projects/seta-web/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/seta-web/src/favicon.ico",
              "projects/seta-web/src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "node_modules/primeng/resources/themes/saga-blue/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/primeflex/primeflex.css",
              "node_modules/primeicons/primeicons.css",
              "projects/seta-web/src/styles.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/popper.js/dist/umd/popper.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/seta-web/src/environments/environment.ts",
                  "with": "projects/seta-web/src/environments/environment.prod.ts"
                }
              ],
              "baseHref": "/seta-ui/",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "6mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "dev": {
              "baseHref": "/seta-ui/",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "6mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "test": {
              "fileReplacements": [
                {
                  "replace": "projects/seta-web/src/environments/environment.ts",
                  "with": "projects/seta-web/src/environments/environment.seta-test.ts"
                }
              ],
              "baseHref": "/seta-ui/",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "6mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "docker": {
              "fileReplacements": [
                {
                  "replace": "projects/seta-web/src/environments/environment.ts",
                  "with": "projects/seta-web/src/environments/environment.seta-docker.ts"
                }
              ],
              "baseHref": "/seta-ui/",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "6mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            }
            
          },
          "aot": true
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "seta-web:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "seta-web:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "seta-web:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/seta-web/src/test.ts",
            "polyfills": "projects/seta-web/src/polyfills.ts",
            "tsConfig": "projects/seta-web/tsconfig.spec.json",
            "karmaConfig": "projects/seta-web/karma.conf.js",
            "assets": [
              "projects/seta-web/src/favicon.ico",
              "projects/seta-web/src/assets"
            ],
            "styles": [
              "projects/seta-web/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/seta-web/tsconfig.app.json",
              "projects/seta-web/tsconfig.spec.json",
              "projects/seta-web/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/seta-web/e2e/protractor.conf.js",
            "devServerTarget": "seta-web:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "seta-web:serve:production"
            }
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "ea1fd6a3-6540-44b1-9f7c-4223c38ee491"
  },
  "defaultProject": "seta-web"
}
